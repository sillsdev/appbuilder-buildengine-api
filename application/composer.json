{
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "^2.0.15",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "cpliakas/git-wrapper": "*",
        "chbiel/jenkins-php-api": "*",
        "aws/aws-sdk-php": "^3.56",
        "silinternational/yii2-jsonsyslog": "*"
    },
    "require-dev": {
        "silinternational/yii2-codeception": "dev-master",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-faker": "*",
        "fillup/phpmyadmin-minimal": "4.2.2",
        "codeception/codeception": "2.0.*",
        "codeception/specify": "0.*",
        "codeception/verify": "0.*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
